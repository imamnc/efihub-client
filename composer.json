{
  "name": "efihub/client",
  "description": "Laravel client for EFIHUB Integration Service",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Efihub\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Efihub\\EfihubServiceProvider"
      ],
      "aliases": {
        "Efihub": "Efihub\\Facades\\Efihub"
      }
    }
  }
}
